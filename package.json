{
  "name": "@1024pix/ember-api-actions",
  "version": "1.0.0",
  "description": "Trigger remote actions that don't fit into CRUD, in your Ember.js apps",
  "keywords": [
    "ember-addon",
    "api",
    "data",
    "request",
    "ajax",
    "model"
  ],
  "homepage": "https://github.com/1024pix/ember-api-actions",
  "repository": "https://github.com/1024pix/ember-api-actions.git",
  "license": "MIT",
  "author": "Mike North <michael.l.north@gmail.com> (https://mike.works), 1024pix team",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint": "npm run lint:ts && npm run lint:hbs",
    "lint:hbs": "ember-template-lint .",
    "lint:ts": "tslint --project .",
    "prepare": "husky install",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean",
    "start": "ember server",
    "test": "ember test"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "ember-cli-babel": "^7.1.3",
    "ember-cli-typescript": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.6",
    "@commitlint/config-conventional": "8.3.6",
    "@commitlint/travis-cli": "8.3.6",
    "@ember/optional-features": "^1.3.0",
    "@ember/test-helpers": "2.5.0",
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.0.0",
    "@types/ember": "3.16.14",
    "@types/ember__test-helpers": "1.7.6",
    "@types/ember-data": "3.16.25",
    "@types/ember-qunit": "3.4.15",
    "@types/ember-test-helpers": "^1.0.16",
    "@types/qunit": "2.11.3",
    "@types/rsvp": "4.0.9",
    "broccoli-asset-rev": "3.0.0",
    "ember-auto-import": "^2.7.2",
    "ember-cli": "4.11.0",
    "ember-cli-code-coverage": "1.0.3",
    "ember-cli-dependency-checker": "3.3.2",
    "ember-cli-github-pages": "0.2.2",
    "ember-cli-htmlbars": "6.2.0",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-test-loader": "3.0.0",
    "ember-cli-tslint": "0.1.4",
    "ember-cli-typescript-blueprints": "3.0.0",
    "ember-cli-uglify": "3.0.0",
    "ember-code-snippet": "2.4.2",
    "ember-data": "3.27.1",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-export-application-global": "2.0.1",
    "ember-load-initializers": "2.1.2",
    "ember-maybe-import-regenerator": "0.1.6",
    "ember-qunit": "5.1.5",
    "ember-resolver": "8.1.0",
    "ember-source": "4.11.0",
    "ember-source-channel-url": "3.0.0",
    "ember-template-lint": "1.14.0",
    "ember-try": "1.4.0",
    "husky": "6.0.0",
    "json-typescript": "1.1.2",
    "jsonapi-typescript": "0.1.3",
    "loader.js": "4.7.0",
    "pretender": "^3.4.7",
    "qunit": "2.16.0",
    "qunit-dom": "2.0.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.2.4"
  },
  "engines": {
    "node": "10.* || >= 12.*"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://mike.works/ember-api-actions"
  },
  "engine": {
    "node": "20.13.1"
  }
}
